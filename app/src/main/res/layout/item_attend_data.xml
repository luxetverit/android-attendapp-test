<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:id="@+id/attend_item">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/attend_item_guide1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.07"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/attend_item_guide2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.93"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintWidth_percent="0.86"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/attend_item_guide1"
        app:layout_constraintRight_toRightOf="@id/attend_item_guide2"
        app:layout_constraintBottom_toBottomOf="parent">

        <TextView
            android:id="@+id/attend_id"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textSize="15dp"
            android:textStyle="normal"
            android:textColor="#5c5c5c"
            android:lineSpacingExtra="4.5sp"
            android:gravity="center"
            android:paddingLeft="2dp"
            android:background="@drawable/btn_border_left_gray2"
            app:layout_constraintWidth_percent="0.15"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/attend_name"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/attend_name"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textSize="15dp"
            android:textStyle="normal"
            android:textColor="#5c5c5c"
            android:lineSpacingExtra="4.5sp"
            android:gravity="center"
            android:paddingLeft="2dp"
            android:background="@drawable/btn_border_horizontal_gray2"
            app:layout_constraintWidth_percent="0.3"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toLeftOf="@id/attend_nat"
            app:layout_constraintLeft_toRightOf="@id/attend_id"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/attend_nat"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textSize="15dp"
            android:textStyle="normal"
            android:textColor="#5c5c5c"
            android:lineSpacingExtra="4.5sp"
            android:gravity="center"
            android:paddingRight="2dp"
            android:background="@drawable/btn_border_right_gray2"
            app:layout_constraintWidth_percent="0.15"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toLeftOf="@id/attend_date"
            app:layout_constraintLeft_toRightOf="@id/attend_name"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/attend_date"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textSize="15dp"
            android:textStyle="normal"
            android:textColor="#5c5c5c"
            android:lineSpacingExtra="4.5sp"
            android:gravity="center"
            android:paddingRight="2dp"
            android:background="@drawable/btn_border_right_gray2"
            app:layout_constraintWidth_percent="0.2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toLeftOf="@id/attend_data"
            app:layout_constraintLeft_toRightOf="@id/attend_nat"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/attend_data"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textSize="15dp"
            android:textStyle="normal"
            android:textColor="#5c5c5c"
            android:lineSpacingExtra="4.5sp"
            android:gravity="center"
            android:paddingRight="2dp"
            android:background="@drawable/btn_border_right_gray2"
            app:layout_constraintWidth_percent="0.15"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/attend_date"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>